{% extends 'base.html.twig' %}

{% block title %}
    Mise à jour type de plat
{% endblock %}

{% block body %}
        <div class="container p-3 my-3 border">

        <h1>Mise à jour d'un type de plat</h1>
        {% form_theme form 'bootstrap_4_layout.html.twig' %}
        {{ form_start(form) }}
        {{ form_label(form.libelleType,'Libellé du type de plat') }} {{ form_widget(form.libelleType) }}
        <br>
        <input type="submit" class="btn btn-primary"/>
        {{ form_end(form) }}
    </div>
{% endblock %}